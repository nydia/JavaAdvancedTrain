spring:
  application:
    name: springcloud-server
  profiles:
    active: dev

---
spring:
  config:
    activate:
      on-profile: local
  cloud:
    nacos:
      discovery:
        #server-addr: 192.168.41.82:8848,192.168.41.83:8848,192.168.41.84:8848
        #namespace: local
        server-addr: 127.0.0.1:8848
        namespace: public
        enabled: true # 不注册到注册中心，默认true
      config:
        #server-addr: 192.168.41.82:8848,192.168.41.83:8848,192.168.41.84:8848
        server-addr: 127.0.0.1:8848
        file-extension: yaml
        namespace: public
        prefix: ${spring.application.name}

---
spring:
  config:
    activate:
      on-profile: dev
  cloud:
    nacos:
      discovery:
        server-addr: 192.168.43.81:8848,192.168.43.82:8848,192.168.43.83:8848
        namespace: gray
        enabled: true # 不注册到注册中心，默认true
        group: DEFAULT_GROUP
      config:
        server-addr: 192.168.43.81:8848,192.168.43.82:8848,192.168.43.83:8848
        namespace: gray
        file-extension: yaml
        prefix: ${spring.application.name}